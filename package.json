{
    "name": "eleventastic",
    "version": "1.0.0",
    "description": "A boilerplate for eleventy websites",
    "browserslist": [
        "defaults"
    ],
    "scripts": {
        "start": "npm run dev",
        "dev": "run-s clean eleventy:dev --print-label",
        "build": "run-s clean eleventy:prod --print-label",
        "eleventy:dev": "cross-env ELEVENTY_ENV=development eleventy --serve",
        "eleventy:prod": "cross-env ELEVENTY_ENV=production eleventy",
        "clean": "del-cli dist",
        "test": "echo \"Error: no test specified\" && exiÍÍt 1"
    },
    "repository": {
        "type": "git",
        "url": "http://github.com/maxboeck/eleventastic"
    },
    "author": "Max Böck",
    "license": "MIT",
    "dependencies": {
        "@11ty/eleventy": "^0.12.1",
        "@11ty/eleventy-navigation": "^0.1.6",
        "@11ty/eleventy-plugin-rss": "^1.0.9",
        "@babel/core": "^7.11.4",
        "@babel/plugin-transform-runtime": "^7.11.0",
        "@babel/preset-env": "^7.11.0",
        "axios": "^0.21.1",
        "babel-loader": "^8.2.2",
        "clean-css": "^4.2.3",
        "critical": "^2.0.3",
        "cssesc": "^3.0.0",
        "del-cli": "^3.0.1",
        "focus-trap": "^5.1.0",
        "focus-visible": "^5.2.0",
        "html-minifier": "^4.0.0",
        "luxon": "^1.25.0",
        "markdown-it": "^12.0.2",
        "memfs": "^3.2.0",
        "moment": "^2.29.1",
        "node-sass": "^5.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "svg-sprite": "^1.5.0",
        "webpack": "^4.44.1"
    },
    "devDependencies": {
        "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
        "cross-env": "^7.0.2",
        "stylelint": "^13.12.0",
        "stylelint-config-standard": "^21.0.0"
    }
}
